S=1.2

for M in 0.00 0.08 0.12 0.20; do
  ./measure $S $M
  sleep 2200
done

S=1.0

for M in 0.20 0.12 0.08 0.00; do
  ./measure $S $M
  sleep 2200
done

S=1.5

for M in 0.00 0.08 0.12 0.20; do
  ./measure $S $M
  sleep 2200
done

S=2.0

for M in 0.20 0.12 0.08 0.00; do
  ./measure $S $M
  sleep 2200
done


./measure 1.2 0

